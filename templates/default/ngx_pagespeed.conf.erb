<% if @params['canonicalize_javascript_libraries'] %>
include pagespeed_libraries.conf;
<% end %>
pagespeed <%= @params['enabled'] %>;
pagespeed FileCachePath <%= @params['FileCachePath'] %>;
pagespeed EnableFilters <%= @params['EnableFilters'] %>;
<% if @params['DisableFilters'] %>
pagespeed DisableFilters <%= @params['DisableFilters'] %>;
<% end %>